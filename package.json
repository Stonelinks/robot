      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "express": "^4.14.0",
    "ws": "^1.1.0",
    "bufferutil": "1.2.1",
    "express": "4.13.3",
    "foreman": "1.4.1",
    "jsmpeg": "1.0.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-scripts": "0.7.0"
  },
  "scripts": {
    "start": "nf start -p $PORT",
    "dev": "nf start -p 3000 --procfile Procfile.dev",
