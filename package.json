{
  "name": "robot",
  "version": "1.0.0",
  "description": "A hacked together GCS",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Stonelinks/robot.git"
  },
  "author": "Lucas Doyle <lucas.p.doyle@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Stonelinks/robot/issues"
  },
  "homepage": "https://github.com/Stonelinks/robot#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "broadwayjs": "0.0.2",
    "bufferutil": "1.2.1",
    "express": "4.13.3",
    "foreman": "3.0.1",
    "johnny-five": "0.10.6",
    "rc-slider": "5.3.5",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-scripts": "0.7.0",
    "stream-split": "1.1.0",
    "worker-loader": "0.7.1",
    "ws": "1.1.0"
  },
  "scripts": {
    "lint": "standard --fix",
    "start": "nf start -p $PORT",
    "dev": "nf start -p 3000 --procfile Procfile.dev",
    "server": "./node_modules/.bin/babel-node server",
    "start:client": "cd client && react-scripts start",
    "build:client": "cd client && react-scripts build",
    "test:client": "cd client && react-scripts test --env=jsdom",
    "eject:client": "cd client && react-scripts eject"
  },
  "devDependencies": {
    "standard": "8.6.0"
  }
}
